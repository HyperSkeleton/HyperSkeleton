-- Scripts:

local function AXUVAR_fake_script() -- HomeText.Script 
	local script = Instance.new('Script', HomeText)

	script.Parent.MouseEnter:Connect(function()--Checks if mouse is over button
		print("Mouse Entered")
		    wait(0.01)
	        script.Parent.BackgroundTransparency = 0.50
		end)--Ends the part
	
	script.Parent.MouseLeave:Connect(function()--Checks if mouse left button
		print("Mouse Leave")
		    wait(0.01)
			script.Parent.BackgroundTransparency = 1
		end)--Ends the part
end
coroutine.wrap(AXUVAR_fake_script)()
