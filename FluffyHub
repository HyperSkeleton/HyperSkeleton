FluffyHub.Name = "FluffyHub"
FluffyHub.Parent = game:GetService("CoreGui")

wait(1)
game:GetService("StarterGui"):SetCore("SendNotification",{
Title    = "FluffyHub",
Text     = "Use = to hide the hub.",
Duration = 10,
})

